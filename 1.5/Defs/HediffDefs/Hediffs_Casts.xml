			<li Class="HediffCompProperties_DisappearsOnDeath"/>
		</comps>
		<isBad>false</isBad>
		<stages>
			<li>
				<statOffsets>
					<WorkSpeedGlobal>0.2</WorkSpeedGlobal>
					<PlantWorkSpeed>0.25</PlantWorkSpeed>
					<AnimalGatherSpeed>0.25</AnimalGatherSpeed>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>
</Defs>
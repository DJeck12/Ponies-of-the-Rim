<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<!-- Batpony -->

	<RulePackDef>
		<defName>NameGeneratorBatpony</defName>
		<include>
			<li>NameGeneratorPony</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->[Bat_First] [Bat_Last]</li>
				<li>r_name->[Bat_First] [Bat_Word]</li>
				<li>r_name->[Bat_Word] [Bat_Last]</li>

				<li>r_name->[First] [Bat_Last]</li>
				<li>r_name->[First] [Bat_Word]</li>
				<li>r_name->[Word] [Bat_Last]</li>

				<li>r_name->[Bat_First] [Last]</li>
				<li>r_name->[Bat_First] [Word]</li>
				<li>r_name->[Bat_Word] [Last]</li>
			</rulesStrings>
			<rulesFiles>
				<li>Bat_First->Pony_Names/Name_Bat_First</li>
				<li>Bat_Last->Pony_Names/Name_Bat_Last</li>
				<li>Bat_Word->Pony_Names/Name_Bat_Word</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>


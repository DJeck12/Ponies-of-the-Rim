			<li>
				<def>NightOwl</def>
			</li>
		</forcedTraits>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>horse</symbol></li>
				<li><symbol>pon</symbol></li>
				<li><symbol>bat</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>neigh</symbol></li>
				<li><symbol>pony</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

</Defs>
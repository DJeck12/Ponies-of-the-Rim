<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ideology</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PreceptDef[defName="Pony_Earthpony_Respected"]/conflictingMemes</xpath>
					<value>
						<li>Pony_BatponySupremacist</li>
						<li>Pony_ThestralSupremacist</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PreceptDef[defName="Pony_Earthpony_Exalted"]/conflictingMemes</xpath>
					<value>
						<li>Pony_BatponySupremacist</li>
						<li>Pony_ThestralSupremacist</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>
<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/GeneDef[defName="Headbone_MiniHorns"]</xpath>
					<value>
						<exclusionTags>
							<li>MultiHorns</li>
						</exclusionTags>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/GeneDef[defName="Headbone_CenterHorn"]</xpath>
					<value>
						<exclusionTags>
							<li>SingleHorn</li>
						</exclusionTags>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>
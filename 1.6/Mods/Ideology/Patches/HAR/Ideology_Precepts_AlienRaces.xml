<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Humanoid Alien Races</li>
		</mods>
		<match Class="PatchOperationSequence">
			<!-- <success>Always</success> -->
			<operations>
				<li Class="PatchOperationConditional">
					<!-- <success>Always</success> -->
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Abhorrent"]/conflictingMemes</xpath>
					<nomatch Class="PatchOperationAdd">
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Abhorrent"]</xpath>
					<value>
						<conflictingMemes>
							<li>Pony_Friendship</li>
						</conflictingMemes>
					</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Abhorrent"]/conflictingMemes</xpath>
						<value>
							<li>Pony_Friendship</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationConditional">
					<!-- <success>Always</success> -->
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Respected"]/conflictingMemes</xpath>
					<nomatch Class="PatchOperationAdd">
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Respected"]</xpath>
					<value>
						<conflictingMemes>
							<li>Pony_EarthponySupremacist</li>
							<li>Pony_PegasusSupremacist</li>
							<li>Pony_UnicornSupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_BatponySupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_ThestralSupremacist</li>
						</conflictingMemes>
					</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Respected"]/conflictingMemes</xpath>
						<value>
							<li>Pony_EarthponySupremacist</li>
							<li>Pony_PegasusSupremacist</li>
							<li>Pony_UnicornSupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_BatponySupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_ThestralSupremacist</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationConditional">
					<!-- <success>Always</success> -->
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Exalted"]/conflictingMemes</xpath>
					<nomatch Class="PatchOperationAdd">
					<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Exalted"]</xpath>
					<value>
						<conflictingMemes>
							<li>Pony_EarthponySupremacist</li>
							<li>Pony_PegasusSupremacist</li>
							<li>Pony_UnicornSupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_BatponySupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_ThestralSupremacist</li>
						</conflictingMemes>
					</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/PreceptDef[defName="HAR_AlienRaces_Exalted"]/conflictingMemes</xpath>
						<value>
							<li>Pony_EarthponySupremacist</li>
							<li>Pony_PegasusSupremacist</li>
							<li>Pony_UnicornSupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_BatponySupremacist</li>
							<li MayRequire="Pony.PoniesOfTheRim.Batponies">Pony_ThestralSupremacist</li>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>